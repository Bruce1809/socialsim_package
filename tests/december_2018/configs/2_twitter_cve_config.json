{
    "baseline": {
        "node": {
            "content_diffusion_delay": {
                "scale": "node",
                "node_type": "content",
                "measurement": "getContentDiffusionDelay",
                "measurement_args": {
                    "eventTypes": [
                        "reply",
                        "retweet",
                        "quote"
                    ],
                    "time_bin": "h",
                    "content_field": "root"
                },
                "metrics": {
                    "ks_test": {
                        "metric": "ks_test",
                        "metric_args": {}
                    },
                    "js_divergence": {
                        "metric": "js_divergence",
                        "metric_args": {
                            "discrete": false,
                            "base": 2.0
                        }
                    }
                }
            },
            "content_growth": {
                "scale": "node",
                "node_type": "content",
                "measurement": "getContentGrowth",
                "measurement_args": {
                    "eventTypes": [
                        "tweet",
                        "retweet",
                        "quote",
                        "reply"
                    ],
                    "time_bin": "h",
                    "content_field": "root"
                },
                "metrics": {
                    "rmse": {
                        "metric": "rmse",
                        "metric_args": {
                            "join": "inner",
