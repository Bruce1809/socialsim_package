                            "relative": false
                        }
                    },
                    "nrmse": {
                        "metric": "rmse",
                        "metric_args": {
                            "join": "inner",
                            "fill_value": 0,
                            "relative": false
                        }
                    },
                    "dtw": {
                        "metric": "dtw",
                        "metric_args": {}
                    }
                }
            },
            "content_contributors": {
                "scale": "node",
                "node_type": "content",
                "measurement": "getContributions",
                "measurement_args": {
                    "eventTypes": [
                        "tweet",
                        "retweet",
                        "quote",
                        "reply"
                    ],
                    "content_field": "root"
                },
                "metrics": {
                    "rmse": {
                        "metric": "rmse",
                        "metric_args": {
                            "join": "inner",
                            "fill_value": 0,
                            "relative": false
                        }
                    },
                    "nrmse": {
                        "metric": "rmse",
                        "metric_args": {
                            "join": "inner",
                            "fill_value": 0,
                            "relative": false
                        }
                    },
                    "dtw": {
                        "metric": "dtw",
                        "metric_args": {}
                    }
                }
            }
        },
        "population": {
            "content_popularity_distribution": {
                "scale": "population",
                "node_type": "content",
                "measurement": "getDistributionOfEventsByContent",
                "measurement_args": {
                    "eventTypes": [
                        "retweet"
                    ],
                    "content_field": "root"
                },
                "metrics": {
                    "js_divergence": {
                        "metric": "js_divergence",
                        "metric_args": {
                            "discrete": false,
                            "base": 2.0
                        }
                    },
                    "rmse": {
                        "metric": "rmse",
                        "metric_args": {
                            "join": "inner",
                            "fill_value": 0,
                            "relative": false
                        }
                    },
                    "nrmse": {
                        "metric": "rmse",
                        "metric_args": {
                            "join": "inner",
                            "fill_value": 0,
                            "relative": false
                        }
                    },
                    "r2": {
                        "metric": "r2",
                        "metric_args": {
                            "join": "inner",
                            "fill_value": 0
                        }
                    }
                }
            },
            "content_popularity_topk": {
                "scale": "population",
                "node_type": "content",
                "measurement": "getTopKContent",
                "measurement_args": {
                    "k": 10,
                    "eventTypes": [
                        "retweet"
                    ],
                    "content_field": "root"
                },
                "metrics": {
                    "rbo": {
                        "metric": "rbo",
                        "metric_args": {
                            "p": 0.95
